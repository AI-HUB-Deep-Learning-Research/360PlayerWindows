<Window x:Class="PlayerUI.Statistics.SendStatistics"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PlayerUI.Statistics"
        mc:Ignorable="d"
        Title="Send Statistics" Height="600" Width="400">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="auto" />
        </Grid.RowDefinitions>
        <WebBrowser x:Name="browser" Grid.Row="0" Margin="4" />
        <Button x:Name="cancel" Grid.Column="0" Grid.Row="1" Margin="4">Cancel</Button>
    </Grid>
</Window>
